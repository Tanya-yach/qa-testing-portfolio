            <stringProp name="script">import groovy.json.JsonSlurper

def respCode = prev.getResponseCode()
def resp = prev.getResponseDataAsString()
def contentType = prev.getContentType()

if (respCode == &apos;200&apos; &amp;&amp; contentType != null &amp;&amp; contentType.contains(&apos;application/json&apos;)) {
    try {
        def json = new JsonSlurper().parseText(resp)
        if (json?.token) {
            vars.put(&apos;auth_ok&apos;,&apos;true&apos;)
            vars.put(&apos;token&apos;, json.token.toString())
        } else {
            vars.put(&apos;auth_ok&apos;,&apos;false&apos;)
            log.warn(&apos;Auth returned JSON but no token: &apos; + resp.take(200))
        }
    } catch(Exception e) {
        vars.put(&apos;auth_ok&apos;,&apos;false&apos;)
        log.warn(&apos;JSON parse failed after auth: &apos; + e.getMessage())
    }
} else {
    vars.put(&apos;auth_ok&apos;,&apos;false&apos;)
    log.warn(&quot;Auth failed: code=${respCode}, contentType=${contentType}, body=${resp.take(200)}&quot;)
}</stringProp>
