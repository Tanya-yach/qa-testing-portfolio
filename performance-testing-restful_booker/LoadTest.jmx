            <stringProp name="script">import groovy.json.JsonSlurper

def response = prev.getResponseDataAsString()

try {
    def json = new JsonSlurper().parseText(response)

    def bookingId = json?.bookingid
    if (bookingId == null) {
        throw new Exception(&quot;Booking ID not found in response: &quot; + response)
    }

    props.put(&quot;bookingid&quot;, bookingId.toString())

    log.info(&quot;OK_Extracted and saved bookingid to props: &quot; + bookingId)
} catch (Exception e) {
    log.error(&quot;Failed to parse JSON: &quot; + e.getMessage())
    throw e 
